{
	"compilerOptions": {
		"jsx": "preserve",
		"baseUrl": "./src",
		"paths": {
			"@Components/*": ["Components/*"],
			"@Hooks/*": ["Hooks/*"],
			"@App/*": ["Components/App/*"],
			"@List/*": ["Components/List/*"],
			"@Item/*": ["Components/List/Item/*"],
			"@InputWithLabel/*": ["Components/InputWithLabel/*"]
		}
	},
	"exclude": ["node_modules", "**/node_modules/*"]
}
